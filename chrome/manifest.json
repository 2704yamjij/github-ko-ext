{
  "manifest_version": 2,
  "name": "GitHub Translator - ko",
  "version": "0.1.0",
  "description": "GitHub 한글화",
  "author": "https://github.com/hatemogi/github-ko",
  "homepage_url": "https://github.com/hatemogi/github-ko",
  "content_scripts": [{
    "matches": ["https://*.github.com/*"],
    "css": ["github-ko.css"],
    "js": ["github-ko.js"]}],
  "permissions": ["https://*.github.com/"],
  "web_accessible_resources": ["fonts/*.ttf"]
}
